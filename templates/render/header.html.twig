    <li class="main-li">{{ dropdownMenu(59, (getActivityobject(59).__toString  | split(' ')[0])) | raw }}</li>
    <li class="main-li">{{ dropdownMenu(4, (getActivityobject(4).__toString  | split(' ')[0]), [], {
            'diverseTransportationOfGoods' : dropdownMenu(87, 'Transport Express', [], null, true)
        }) | raw }}</li>
