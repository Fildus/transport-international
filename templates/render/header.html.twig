    <li class="main-li">{{ dropdownMenu(91, (getActivityobject(91).__toString  | split(' ')[0])) | raw }}</li>
    <li class="main-li">{{ dropdownMenu(66, (getActivityobject(66).__toString  | split(' ')[0])) | raw }}</li>
