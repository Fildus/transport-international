{% for k, domain in domains %}
    {% if k != app.request.locale %}
        <li>
            <a hreflang="{{ domain.domain }}"
            {#href="{{ 'http://test-'~domain.domain~path(route~'.'~k) }}">#}
            href="{{ 'http://'~domain.domain }}">
            <img src="{{ asset('build/img/front/flags/'~domain.img) }}"
                 alt="{{ domain.hreflang }}">
            </a>
        </li>
    {% endif %}
{% endfor %}